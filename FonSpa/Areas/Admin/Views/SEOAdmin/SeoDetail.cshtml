@model Models.Entity.SEO
@{
    ViewBag.Title = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";

}

<section class="wrapper">
    <div class="form-w3layouts">
        <div class="row">
            <div class="col-lg-12">
                <section class="panel">
                    <header class="panel-heading">
                        Seo Detail
                        <span class="tools pull-right">
                        </span>
                    </header>
                    <div class="panel-body">
                        <div class="form">

                            @*<div class="form">*@

                            @*<form  method="get" action="" novalidate="novalidate">*@
                            @using (Html.BeginForm("Edit", "SeoAdmin", FormMethod.Post, new { @class = "cmxform form-horizontal", @id = "signupForm" }))
                            {
                                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                <div class="form-group ">
                                    <label class="control-label col-lg-3">Id</label>
                                    <div class="col-lg-6">
                                        @*<input class="form-control " id="username" name="username" type="text">*@
                                        @Html.EditorFor(model => model.Id, new { htmlAttributes = new { @class = "form-control", @type = "text", @readonly = "readonly" } })
                                    </div>
                                </div>
                                <div class="form-group ">
                                    <label class="control-label col-lg-3">Trang</label>
                                    <div class="col-lg-6">
                                        @Html.EditorFor(model => model.Name, new { htmlAttributes = new { @class = "form-control", @type = "text", @readonly = "readonly" } })
                                    </div>
                                </div>
                                <div class="form-group ">
                                    <label for="MetaTitle" class="control-label col-lg-3">Meta Title</label>
                                    <div class="col-lg-6">
                                        @Html.EditorFor(model => model.MetaTitle, new { htmlAttributes = new { @class = "form-control", @type = "text" } })
                                    </div>
                                </div>
                                <div class="form-group ">
                                    <label class="control-label col-lg-3">Meta Description</label>
                                    <div class="col-lg-6">
                                        @Html.EditorFor(model => model.SeoDescription, new { htmlAttributes = new { @class = "form-control", @type = "text" } })
                                    </div>
                                </div>
                                <div class="form-group ">
                                    <label class="control-label col-lg-3">Meta Keyword</label>
                                    <div class="col-lg-6">
                                        @Html.EditorFor(model => model.SeoKeyWord, new { htmlAttributes = new { @class = "form-control", @type = "text" } })
                                    </div>
                                </div>
                               
                                <div class="form-group">
                                    <div class="col-lg-offset-3 col-lg-6">
                                        <button class="btn btn-primary" type="submit">Save</button>
                                        <form action="/Admin/SeoAdmin/SeoList">
                                            <input class="btn btn-default" type="button" onclick="window.location.href = '/Admin/SeoAdmin/SeoList';" value="Back" />
                                        </form>
                                    </div>
                                </div>
                            }

                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
</section>

